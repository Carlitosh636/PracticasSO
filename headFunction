#include <stdio.h>
#include <stdlib.h>

int headFunction(int num){
	int lineas;
	char buf[1024];
	int cuenta =0;
	if(num <=0){
		lineas=10; //estandar si no se pasa un numero indicado
	}
	else if (num > 0){	
		lineas = num //tomamos el numero de lineas a escribir

	}
	
	while((cuenta<lineas) && (fgets(buf, 1024, stdin) != NULL)){
		printf("%s",buf);
		cuenta++;
	}
	printf("%s",buf);
	return 0;
}
